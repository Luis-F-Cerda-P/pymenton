<div id="<%= dom_id shipment_receipt %>">
  <p>
    <strong>Order item:</strong>
    <%= shipment_receipt.order_item_id %>
  </p>

  <p>
    <strong>Inventory item:</strong>
    <%= shipment_receipt.inventory_item_id %>
  </p>

  <p>
    <strong>Shipment:</strong>
    <%= shipment_receipt.shipment_id %>
  </p>

  <p>
    <strong>Shipment package:</strong>
    <%= shipment_receipt.shipment_package_id %>
  </p>

  <p>
    <strong>Quantity accepted:</strong>
    <%= shipment_receipt.quantity_accepted %>
  </p>

</div>
